@model IQueryable<Processor>

@{
    TextField textFiel = ViewBag.TextField;

    ViewBag.Title = textFiel.Title;
    ViewBag.Text = textFiel.Text;
}

<div class="div-box">
    @if (Model.Any())
    {
    <ul class="big-image-list">
        @foreach (Processor item in Model)
        {
        <li>
            <a asp-area="" asp-controller="Processors" asp-action="Index" asp-route-id="@item.Id">
                <img src="~/img/@item.TitleImagePath" class="left" />
            </a>
            <h4>@item.Title</h4>
            <p>@item.Subtitle</p>
            <a asp-area="" asp-controller="Processors" asp-action="GetMotherboardsBySocket" asp-route-id="@item.Id"> Совместимая материнская плата </a>
        </li>
        }
    </ul>
    }
</div>


